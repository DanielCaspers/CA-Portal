{
  "name": "murphy-automotive-apps",
  "version": "0.0.6",
  "scripts": {
    "ng": "ng",
    "start:default": "ng serve",
    "build:default": "ng build",
    "lint": "ng lint",
    "install-with-legacy": "npm install --legacy-peer-deps",
    "report:serve:local": "ng serve InspectionReport-Public --port 4200",
    "report:serve:staging": "ng serve InspectionReport-Public -c staging --port 4200",
    "report:serve:prod": "ng serve InspectionReport-Public -c production --port 4200",
    "report:build:local": "npm run sharedlib:build && ng build InspectionReport-Public",
    "report:build:staging": "npm run sharedlib:build && ng build InspectionReport-Public -c staging",
    "report:build:prod": "npm run sharedlib:build && ng build InspectionReport-Public -c production",
    "report:lint": "ng lint InspectionReport-Public",
    "report-internal:serve:local": "ng serve InspectionReport-Internal --port 4202",
    "report-internal:serve:staging": "ng serve InspectionReport-Internal -c staging --port 4202",
    "report-internal:serve:prod": "ng serve InspectionReport-Internal -c production --port 4202",
    "report-internal:build:local": "npm run sharedlib:build && ng build InspectionReport-Internal",
    "report-internal:build:staging": "npm run sharedlib:build && ng build InspectionReport-Internal -c staging",
    "report-internal:build:prod": "npm run sharedlib:build && ng build InspectionReport-Internal -c production",
    "report-internal:lint": "ng lint InspectionReport-Internal",
    "mycar:serve:local": "ng serve MyCar --port 4201",
    "mycar:serve:staging": "ng serve MyCar -c staging --port 4201",
    "mycar:serve:prod": "ng serve MyCar -c production --port 4201",
    "mycar:build:local": "npm run sharedlib:build && ng build MyCar",
    "mycar:build:staging": "npm run sharedlib:build && ng build MyCar -c staging",
    "mycar:build:prod": "npm run sharedlib:build && ng build MyCar -c production",
    "mycar:lint": "ng lint MyCar",
    "sharedlib:build": "ng build MurphyAutomotive-SharedLibrary",
    "sharedlib:serve": "ng build MurphyAutomotive-SharedLibrary --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "~13.3.1",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/flex-layout": "~13.0.0-beta.38",
    "@angular/forms": "~13.3.0",
    "@angular/material": "~13.3.1",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@auth0/angular-jwt": "~5.0.2",
    "color-convert": "~2.0.1",
    "core-js": "~3.21.1",
    "hammerjs": "~2.0.8",
    "lodash-es": "~4.17.21",
    "ng2-search-filter": "~0.5.1",
    "rxjs": "6.6.7",
    "rxjs-compat": "6.6.7",
    "swiper": "~8.0.7",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "codelyzer": "~6.0.2",
    "ng-packagr": "~13.3.0",
    "ts-node": "~10.7.0",
    "tsickle": "~0.46.0",
    "tslib": "~2.3.1",
    "tslint": "~6.1.3",
    "typescript": "~4.5.5"
  },
  "optionalDependencies": {
    "murphy-automotive-shared-library": "file:dist/murphy-automotive-shared-library"
  }
}
